#Square pattern
n = int(input())
for i in range(1,n+1):
    for j in range(1,n+1):
        print(n,end="")
    print()

#triangle pattern
def nTriangle(n:int) ->None:
    # Write your solution here.
    for i in range(1,n+1):
        for j in range(1,i+1):
            print(j,end=" ")
        print()
    pass

#pyramid pattern
def printPattern(n):
    for i in range(1,n+1):
        for k in range(n-1,i-1,-1):
            print(end=" ")
        for j in range(1,2*i):
            print("*",end="")
        print()


